/* Slider Stylesheet */
QSlider {
    background: transparent; /* Transparent background */
    border: none; /* No border */
}

/* Slider Groove (Track) */
QSlider::groove:horizontal {
    background: #555555; /* Gray track background */
    height: 6px; /* Height of the track */
    border-radius: 3px; /* Rounded corners */
}

/* Slider Handle */
QSlider::handle:horizontal {
    background: #3498DB; /* Blue handle background */
    width: 16px; /* Width of the handle */
    margin: -8px 0; /* Center the handle vertically */
    border-radius: 8px; /* Round handle corners */
}

/* Slider Handle Hovered State */
QSlider::handle:horizontal:hover {
    background: #2581C0; /* Darker blue on hover */
}

/* Slider Handle Pressed State */
QSlider::handle:horizontal:pressed {
    background: #1B6CAB; /* Even darker blue when pressed */
}















/* Checkbox Stylesheet for Dark Background (Enhanced) */
QCheckBox {
    background: transparent; /* Transparent background */
    spacing: 5px; /* Spacing between the checkbox and text */
    font-size: 14px; /* Font size */
    font-weight: bold; /* Bold text */
    color: #FFFFFF; /* White text for contrast */
}

/* Checkbox Indicator (Unchecked) */
QCheckBox::indicator {
    width: 16px; /* Width of the indicator */
    height: 16px; /* Height of the indicator */
    border: 2px solid #FFFFFF; /* White border for contrast */
    border-radius: 3px; /* Rounded indicator */
}

/* Checkbox Indicator (Checked) */
QCheckBox::indicator:checked {
    background: #3498DB; /* Blue background when checked */
    border: 2px solid #FFFFFF; /* White border when checked */
}

/* Checkbox Indicator (Hovered) */
QCheckBox::indicator:hover {
    border: 2px solid #FFFFFF; /* White border on hover for contrast */
}

/* Checkbox Text (Unchecked) */
QCheckBox::indicator:unchecked {
    background: transparent; /* Transparent background for unchecked */
    border-radius: 3px; /* Rounded indicator */
}

/* Checkbox Text (Checked) */
QCheckBox::indicator:checked:hover {
    border: 2px solid #FFFFFF; /* White border on hover when checked */
}

/* Checkbox Text (Hovered) */
QCheckBox::indicator:unchecked:hover {
    border: 2px solid #FFFFFF; /* White border on hover for unchecked */
}

/* Checkbox Text (Disabled) */
QCheckBox:disabled {
    color: #888888; /* Dimmed text for disabled */
}

/* Checkbox Indicator (Disabled) */
QCheckBox::indicator:unchecked:disabled {
    background: transparent; /* Transparent background for unchecked when disabled */
    border: 2px solid #777777; /* Slightly darker gray border for unchecked when disabled */
}

QCheckBox::indicator:checked:disabled {
    background: #999999; /* Light gray background when checked and disabled */
}













